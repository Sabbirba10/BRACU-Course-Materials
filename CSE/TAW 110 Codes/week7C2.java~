import java.util.Scanner;
public class week6C1
{
  public static void main(String[]args)
  {
    Scanner sc = new Scanner(System.in);  
    int div = 0;
    while(true){
      System.out.println("Enter a number");
      int num = sc.nextInt();
      if(num>0 && num%2==0){
        div = 0;
        for(int i=1; i<=num; i++){
          if(num%i==0){
            div++;
          }
        }
        System.out.println(num+" has "+div+" divisors");
      }
      else{
        break;
      }
    }
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
